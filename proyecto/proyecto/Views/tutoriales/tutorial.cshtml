@model proyecto.Models.tutoriales

@{
    ViewBag.Title = "tutoriales";
}

<h4 id="ps">PUBLICAR TUTORIAL</h4>
<h4 id="ps">Porfavor ingrese todos los datos</h4>
<h5 id = "ps">si ustede desea visualizar sus tutoriales vea @Html.ActionLink(" aqui ","mostrartutorial") </h5  >
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

<form enctype="multipart/form-data" method="post" action="@Url.Action("insertar")">
    @Html.ValidationSummary(true, "Account creation was unsuccessful. Please correct the errors and try again.")
    <div>
        <fieldset id="registrarr3">
            <legend id="hhh">Registrar Datos aqui</legend>
            @{Html.RenderPartial("FormPublicacion");}
            <div>
                @Html.LabelFor(m => m.categoria)
            </div>
            <div>
                @Html.TextBoxFor(m => m.categoria)
                @Html.ValidationMessageFor(m => m.categoria)
            </div> 
             <div>
                @Html.LabelFor(m => m.contenido)
            </div>
            <div>
                @Html.TextAreaFor(m => m.contenido, new { @class = "ckeditor" })
            </div>
    
            <p>
                <input type="submit" value="Register" /  class="button">
            </p>
            

        </fieldset>
    </div>
</form>
