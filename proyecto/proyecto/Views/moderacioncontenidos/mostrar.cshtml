@{
    ViewBag.Title = "mostrar";
}

<h4 id="ps">PUBLICACIONES PENDIENTES POR REVISAR</h4><br/>

<div class="container_24">
    	    <div class="row">
      	    <div class="wrapper">
        	    <div class="grid_16">
          	    <div class="box">
                  <div class="wrapper">
                       <h4>Usted tiene (@ViewBag.cont) publicaciones de libros por revisar pendientes por revisar</h4>     
                       @foreach (var item in ViewBag.mostrar)
                       {
                         
                           string titulo = @item.titulo;
                           int idpub = @item.id;
                           //Guid idus = @item.UserId;
                           <h4 class="alt"> @Html.ActionLink(titulo, "detalle", new { id = idpub})</h4>
                             <text> @item.fecha_publicacion</text><br/>
                                     
                           
                       }

                  </div>
                </div>
              </div>
        </div>
        </div>
</div>
